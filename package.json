    "lint": "prettier \"**/*.+(ts*|js*)\" --write && tslint -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "bundle-analyzer": "WEBPACK_BUNDLE_ANALYZER=true yarn build:prod"
